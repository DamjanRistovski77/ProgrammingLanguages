package exe1;

public class myClass {
	static class myNumber{
	private int num;
	public int num2;
	public myNumber() {
	public myNumber(int num, int num2) {	num= 0;
		num2=100;
	}
	int getNum() {
		return num;
	}
	int getNum2() {
		return num2;
	}
	void setNum (int i) {
		num= i;
		num2= 10*i;
	}
	void description() {
		
	}
	float average() {
		return (num+num2)/2;
	}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
myNumber A=new myNumber();
System.out.println (A.getNum () ) ;
A.setNum (10);
System.out.println(A.getNum());
System.out.println(A.num2);
	}
	
}
	

